{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [".env", "tsconfig.json"],
  "globalEnv": ["ALCHEMY_SECRET_KEY", "SUPABASE_API_KEY", "HASURA_API_URL", "HASURA_API_KEY", "APP_ID"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "test": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "generate": {
      "outputs": ["**/generated.tsx"],
      "env": ["HASURA_API_URL", "HASURA_API_KEY"],
      "cache": false
    }
  }
}
